{
  "kernel_name": "YoloPostProcess",
  "description": "Postprocessing kernel for YOLO v2/v3 network",
  "kernel_type": "cpp",
  "device_type": "cpu",
  "kernel_lib" : "libs/libYoloPostProcess.so",

  "param_list" : {
    "yolo_version": {"type": "int"},
    "net_h":        {"type": "int"},
    "net_w":        {"type": "int"},
    "num_classes":  {"type": "int"},
    "anchor_cnt":   {"type": "int"},
    "conf_thresh":  {"type": "float"},
    "iou_thresh":   {"type": "float"},
    "input_layout": {"type": "string"},
    "biases":       {"type": "float_array"}
  }
}
