{
    "gui": true, 
    "name": "Xilinx Multi-source Dense Similarity Test", 
    "description": "", 
    "flow": "vitis", 
    "platform_whitelist": [
        "u200"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u200": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200_u250.cfg"
                    ]
                }
            }
        }
    }, 
    "data": [
        "./data"
    ], 
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/denseSimilarityKernel.xclbin -weight PROJECT/data/cosine_dense_weight.csr -source PROJECT/data/cosine_dense_source.csr -golden LIB_DIR/L2/tests/dense_similarity_multi_source/data/cosine_dense.mtx -similarityType 1 -dataType 0 -graphType 1 -batchNUM 6 -topK 32 -v 160 -e 200", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/tests/dense_similarity_multi_source/host/test_similarity.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include/hw", 
                "LIB_DIR/L2/include/hw", 
                "LIB_DIR/L2/tests/dense_similarity_multi_source/host", 
                "LIB_DIR/L2/tests/dense_similarity_multi_source/kernel",
                "LIB_DIR/../utils/L1/include", 
                "LIB_DIR/ext/xcl2"
            ], 
            "options": "-O3 "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw", 
                "LIB_DIR/L2/include/hw", 
                "LIB_DIR/L2/tests/dense_similarity_multi_source/kernel"
            ]
        } 
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/dense_similarity_multi_source/kernel/denseSimilarityKernel.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=denseSimilarityKernel", 
                    "name": "denseSimilarityKernel", 
                    "num_compute_units": 1, 
                    "compute_units": [
                        {
                            "name": "denseSimilarityKernel", 
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "config", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "sourceWeight", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "resultID", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "similarity", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "dataIn0", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "dataIn1", 
                                    "memory": "DDR[1]"
                                }, 
                                {
                                    "name": "dataIn2", 
                                    "memory": "DDR[2]"
                                }, 
                                {
                                    "name": "dataIn3", 
                                    "memory": "DDR[3]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "denseSimilarityKernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 800, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
