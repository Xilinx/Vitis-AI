{
    "gui": true,
    "name": "jks.L2.benchmark.twoHop", 
    "description": "", 
    "flow": "vitis", 
    "platform_whitelist": [
        "u50"    
    ], 
    "platform_blacklist": [
        "zc"
    ],
    "platform_properties": {
        "u50": {
            "v++": {
	            "compiler": {
		            "cflags": [
		                "--config PROJECT/conn_u50.cfg"
		            ]
		        }
            }
        }
    },
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/twoHop_kernel.xclbin --offset PROJECT/data/data-csr-offset.mtx --index PROJECT/data/data-csr-indicesweights.mtx --pair PROJECT/data/data-pair.mtx --golden PROJECT/data/data-golden.twoHop.mtx", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/benchmarks/twoHop/host/main.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L2/benchmarks/twoHop/host", 
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include"
            ], 
            "options": "-O3 "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/L2/benchmarks/twoHop/kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/benchmarks/twoHop/kernel/twoHop_kernel.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=twoHop_kernel", 
                    "name": "twoHop_kernel",
                    "num_compute_units": 1, 
                    "compute_units": [
                        {
                            "name": "twoHop_kernel", 
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "pair", 
                                    "memory": "HBM[0]"
                                }, 
                                {
                                    "name": "offsetOneHop", 
                                    "memory": "HBM[2]"
                                }, 
                                {
                                    "name": "indexOneHop", 
                                    "memory": "HBM[3]"
                                }, 
                                {
                                    "name": "offsetTwoHop", 
                                    "memory": "HBM[4]"
                                }, 
                                {
                                    "name": "indexTwoHop", 
                                    "memory": "HBM[5]"
                                }, 
                                {
                                    "name": "cnt_res", 
                                    "memory": "HBM[1]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "twoHop_kernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": 300
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
