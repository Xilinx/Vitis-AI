{
    "gui": false, 
    "name": "Xilinx Label Propagation Benchmark", 
    "description": "", 
    "flow": "vitis", 
    "platform_whitelist": [
        "u50", 
        "u200", 
        "u250", 
        "aws-vu9p-f1", 
        "vck190"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u50": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u50.cfg"
                    ]
                }
            }
        }, 
        "u200": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200_u250.cfg"
                    ]
                }
            }
        }, 
        "aws-vu9p-f1": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200_u250.cfg"
                    ]
                }
            }
        },
        "u250": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200_u250.cfg"
                    ]
                }
            }
        }, 
        "vck190": {
            "host": {
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib", 
                        "${SYSROOT}/opt/xilinx/xrt/lib"
                    ]
                }, 
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include"
                    ], 
                    "options": "--sysroot=$(SYSROOT)"
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_vck190.cfg"
                    ]
                }
            },
            "containers": [
                {
                    "frequency": 150.0,
                    "name": "LPKernel"
                }
            ]
        }
    },
    "data":[
        "PROJECT/data/csr_offsets.txt",
        "PROJECT/data/csr_columns.txt",
        "PROJECT/data/label.txt"
    ],
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/LPKernel.xclbin -o PROJECT/data/csr_offsets.txt -i PROJECT/data/csr_columns.txt -label PROJECT/data/label.txt", 
            "name": "generic launch for all flows"
        },
        {
            "cmd_args": " -xclbin LPKernel.xclbin -o csr_offsets.txt -i csr_columns.txt -label label.txt", 
            "platform": "vck190",
            "name": "vck 190 launch"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/benchmarks/label_propagation/host/main.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/benchmarks/label_propagation/host", 
                "LIB_DIR/L2/benchmarks/label_propagation/kernel", 
                "LIB_DIR/ext/xcl2"
            ], 
            "options": "-O3 "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/benchmarks/label_propagation/kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/benchmarks/label_propagation/kernel/label_propagation_top.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=LPKernel", 
                    "name": "LPKernel", 
                    "num_compute_units": 1, 
                    "compute_units": [
                        {
                            "name": "LPKernel", 
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "offsetCSR", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "indexCSR", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "offsetCSC", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "indexCSC", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "indexCSC2", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "pingHashBuf", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "pongHashBuf", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "labelPing", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "labelPong", 
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "LPKernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
