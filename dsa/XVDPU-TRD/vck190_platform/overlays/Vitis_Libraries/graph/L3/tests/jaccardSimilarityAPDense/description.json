{
    "gui": false, 
    "name": "Xilinx All Pair Dense Matrix Jaccard Similarity ", 
    "description": "", 
    "flow": "vitis", 
    "platform_whitelist": [
        "u50"
    ], 
    "platform_blacklist": [], 
    "platform_properties": {
        "u50": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u50_u280.ini"
                    ]
                }
            }
        }
    }, 
    "launch": [
        {
            "cmd_args": "-weight PROJECT/data/jaccard_dense_weight.csr -golden PROJECT/data/jaccard_dense.mtx -topK 32 ", 
            "ld_library_path": "PROJECT/../../lib:$(XILINX_XRT)/lib:$(XILINX_XRM)/lib", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L3/tests/jaccardSimilarityAPDense/test_jaccardSimilarityAPDense.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L3/include",
                "LIB_DIR/../utils/L1/include"
            ], 
            "options": "-O3 -I$(XILINX_XRM)/include"
        }, 
        "linker": {
            "librarypaths": [
                "LIB_DIR/L3/lib"
            ], 
            "libraries": [
                "graphL3"
            ], 
            "options": "-L$(XILINX_XRM)/lib -lxrm "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw", 
                "LIB_DIR/L2/include/hw", 
                "LIB_DIR/L2/tests/dense_similarity/kernel"
            ]
        } 
    }, 
