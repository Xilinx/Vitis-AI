{
    "name": "Xilinx Filter2D AIE Test",
    "description": "Filter2D AIE function example using mm2s s2mm data mover",
    "flow": "versal",
    "platform_whitelist": [
        "vck190"
    ],
    "platform_blacklist": [],
    "platform_properties": {
        "vck190": {
            "v++": {
                "compiler": {
                    "cflags": [
                        ""
                    ]
                },
                "linker": {
                    "cflags": [
                        "--config PROJECT/system.cfg"
                    ]
                },
                "package": {
                    "cflags": [
                        "--package.defer_aie_run"
                    ]
                }
            }
        }
    },
    "gui": false,
    "launch": [
        {
            "name": "hwemu-launch",
            "target": "hw_emu",
            "pkg-dir": "$(WORK_DIR)",
            "input-dir": "./",
            "output-dir": "./"
        },
        {
            "name": "aiesim-launch",
            "target": "aiesim",
            "pkg-dir": "$(WORK_DIR)",
            "input-dir": "./",
            "output-dir": "./"
        },
        {
            "name": "x86-launch",
            "target": "x86sim",
            "pkg-dir": "$(WORK_DIR)",
            "input-dir": "./",
            "output-dir": "./"
        }
    ],
    "host": {
        "host_exe": "host.elf",
        "compiler": {
            "sources": [
                "LIB_DIR/L2/tests/aie/Filter2D/32bit_aie/host.cpp",
                "./Work/ps/c_rts/aie_control_xrt.cpp"
            ],
            "includepaths": [
                "$(SYSROOT)/usr/include/xrt/",
                "./"
            ],
            "options": "--sysroot=$(SYSROOT)",
            "symbols": ["__PS_ENABLE_AIE__"]
        },
        "linker": {
            "librarypaths": [
                "$(SYSROOT)/usr/lib/"
            ],
            "options": "--sysroot=$(SYSROOT)"
        }
    },
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/aie"
            ]
        }
    },
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/aie/Filter2D/32bit_aie/graph.cpp",
                    "pl-freq": 100,
                    "workdir": "./Work",
                    "option": "-pl-axi-lite=true",
                    "name": "libadf.a",
                    "aie_kernel_files":["xf_filter2d.cc"]
                }
            ]
        }
    ],
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/aie/Filter2D/32bit_aie/mm2s.cpp",
                    "frequency": 100.0,
                    "clflags": " ",
                    "name": "mm2s"
                },
                {
                    "location": "LIB_DIR/L2/tests/aie/Filter2D/32bit_aie/s2mm.cpp",
                    "frequency": 100.0,
                    "clflags": " ",
                    "name": "s2mm"
                }
            ],
            "name": "kernel"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": {
                    "vitis_aie_sim": 50,
                    "vitis_aie_x86sim": 50,
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 200,
                    "vitis_hw_run": 30
                }
            }
        ],
        "targets": [
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
