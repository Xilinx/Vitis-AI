{
    "name": "Xilinx Preprocess AIE Test",
    "description": "Preprocess AIE function example",
    "flow": "versal",
    "platform_whitelist": [
        "vck190"
    ],
    "platform_blacklist": [],
    "platform_properties": {
        "vck190": {
            "v++": {
                "compiler": {
                    "cflags": [
                        ""
                    ]
                },
                "linker": {
                    "cflags": [
                        "--clock.freqHz 400000000:Tiler_top_1",
                        "--clock.freqHz 400000000:stitcher_top_1",
                        "--clock.defaultTolerance 0.001",
                        "--config PROJECT/system.cfg"
                    ]
                },
                "package": {
                    "cflags": [
                        "--package.defer_aie_run"
                    ]
                }
            }
        }
    },
    "data": [
        "LIB_DIR/data/4k.jpg",
        "LIB_DIR/data/HD.jpg"
    ],
    "gui": false,
    "launch": [
        {
            "name": "hwemu-launch",
            "target": "hw_emu",
            "pkg-dir": "$(WORK_DIR)",
            "input-dir": "./",
            "output-dir": "./"
        },
        {
            "name": "aiesim-launch",
            "target": "aiesim",
            "pkg-dir": "$(WORK_DIR)",
            "input-dir": "./",
            "output-dir": "./"
        },
        {
            "name": "x86-launch",
            "target": "x86sim",
            "pkg-dir": "$(WORK_DIR)",
            "input-dir": "./",
            "output-dir": "./"
        }
    ],
    "host": {
        "host_exe": "host.elf",
        "compiler": {
            "sources": [
                "LIB_DIR/L2/tests/aie/preprocess/16bit_aie_8bit_pl/meansub/host.cpp",
                "./Work/ps/c_rts/aie_control_xrt.cpp"
            ],
            "includepaths": [
                "$(SYSROOT)/usr/include/xrt/",
                "./"
            ],
            "options": "--sysroot=$(SYSROOT)",
            "symbols": ["__PS_ENABLE_AIE__"]
        },
        "linker": {
            "librarypaths": [
                "$(SYSROOT)/usr/lib/"
            ],
            "options": "--sysroot=$(SYSROOT)"
        }
    },
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/aie"
            ]
        }
    },
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/aie/preprocess/16bit_aie_8bit_pl/meansub/graph.cpp",
                    "pl-freq": 100,
                    "workdir": "./Work",
                    "option": "-pl-axi-lite=true",
                    "name": "libadf.a",
                    "aie_kernel_files":["xf_pp_top.cc"]
                }
            ]
        }
    ],
    "containers": [
        {
            "accelerators": [
                {
                    "download_url": "https://www.xilinx.com/bin/public/openDownload?filename=tiler.xo",
                    "name": "tiler",
					"frequency": "300"
                },
                {
                    "download_url": "https://www.xilinx.com/bin/public/openDownload?filename=stitcher.xo",
                    "name": "stitcher",
					"frequency": "300"
                }
            ],
			"frequency": "300",
            "name": "kernel"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": {
                    "vitis_aie_sim": 55,
                    "vitis_aie_x86sim": 55,
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 200,
                    "vitis_hw_run": 30
                }
            }
        ],
        "targets": [
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
