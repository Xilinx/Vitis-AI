{
    "name": "Xilinx GEQRF",
    "description": "QR factorization of general matrix",
    "flow": "vitis",
    "platform_type": "",
    "platform_whitelist": [
        "u50",
        "u250",
        "u200",
        "aws-vu9p-f1",
        "vck190"
    ],
    "platform_blacklist": [
        "zc"
    ],
    "platform_properties": {
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            },
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        },
        "u200": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            },
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200.cfg"
                    ]
                }
            }
        },
        "u50": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_HBM"
                    ]
                }
            },
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u50.cfg"
                    ]
                }
            }
        },
        "vck190": {
            "host": {
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib",
                        "${SYSROOT}/opt/xilinx/xrt/lib"
                    ]
                },
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include"
                    ],
                    "options": "--sysroot=$(SYSROOT)"
                }
            }
        }
    },
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/kernel_geqrf.xclbin",
            "name": "generic launch for all flows"
        }
    ],
    "host": {
        "host_exe": "test_geqrf.exe",
        "compiler": {
            "sources": [
                "test_geqrf.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/ext/MatrixGen",
                "LIB_DIR/../utils/L1/include"
            ],
            "options": "-O3 "
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/ext"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel_geqrf.cpp",
                    "frequency": 300.0,
                    "clflags": " -D KERNEL_NAME=kernel_geqrf_0",
                    "name": "kernel_geqrf_0",
                    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "kernel_geqrf_0_1",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "dataA",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "tau",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ],
            "frequency": 300.0,
            "name": "kernel_geqrf"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 120,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    },
    "gui": false
}
