{
    "gui": false,
    "name": "Xilinx Log Analyzer Demo", 
    "description": "Xilinx Log Analyzer Demo", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u200"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u200": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": []
                }
            }, 
            "containers": [
                {
                    "ldclflags": "--config PROJECT/conn_u200.cfg", 
                    "name": "logAnalyzer"
                }
            ]
        }
    }, 
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/logAnalyzer.xclbin -log PROJECT/data/access.log -dat PROJECT/data/geo.dat -ref PROJECT/data/golden.json", 
            "ld_library_path": [
                "PROJECT/re_compile", 
                "PROJECT/re_compile/lib/lib", 
                "PROJECT/ref_result/libmaxminddb/lib/lib"
            ]
        }
    ], 
    "pre_build": [
        {
            "build_cmd": "make -C PROJECT/re_compile; make -C PROJECT/ref_result/libmaxminddb; make -C PROJECT/ref_result/rapidjson"
        }
    ], 
    "host": {
        "host_exe": "test.exe", 
        "compiler": {
            "sources": [
                "host/main.cpp", 
                "host/log_analyzer.cpp", 
                "host/log_analyzer_config.cpp", 
                "utils_sw/xclhost.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "PROJECT/host", 
                "PROJECT/utils_sw", 
                "PROJECT/re_compile/lib/include", 
                "PROJECT/ref_result", 
                "PROJECT/ref_result/libmaxminddb/lib/include", 
                "PROJECT/ref_result/rapidjson/rapidjson/include", 
                "LIB_DIR/L1/include/sw", 
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include"
            ], 
            "options": "-O3 "
        }, 
        "linker": {
            "librarypaths": [
                "PROJECT/re_compile/lib/lib", 
                "PROJECT/re_compile", 
                "PROJECT/ref_result/libmaxminddb/lib/lib"
            ], 
            "libraries": [
                "onig", 
                "xfcompile", 
                "maxminddb"
            ], 
            "options": ""
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw", 
                "LIB_DIR/L2/include/hw", 
                "PROJECT/kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/re_engine_kernel.cpp", 
                    "frequency": 300, 
                    "name": "reEngineKernel", 
                    "num_compute_units": 4
                }, 
                {
                    "location": "kernel/geoip_kernel.cpp", 
                    "frequency": 300, 
                    "name": "GeoIP_kernel", 
                    "num_compute_units": 1
                }, 
                {
                    "location": "kernel/WJ_kernel.cpp", 
                    "frequency": 300, 
                    "name": "WJ_kernel", 
                    "num_compute_units": 1
                }
            ], 
            "frequency": 300, 
            "name": "logAnalyzer"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": 520
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
