{
    "gui": false,
    "name": "Xilinx RTM Test", 
    "description": "Xilinx RTM Test", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u280"
    ], 
    "platform_blacklist": [
        "other"
    ], 
    "platform_properties": {
        "u280": {
            "containers": [
                {
                    "name": "rtm", 
                    "ldclflags": "--config PROJECT/conn_u280.cfg"
                }
            ]
        }
    }, 
    "launch": [
        {
            "name": "generic launch for all flows", 
            "cmd_args": "BUILD/rtm.xclbin BUILD/data/"
        }
    ], 
    "pre_launch": [
        {
            "launch_cmd": "make data_gen"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "main.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L3/include/sw", 
                "LIB_DIR/L2/include/hw/rtm2d", 
                "LIB_DIR/L2/include/sw/rtm2d", 
                "LIB_DIR/L3/include/sw/rtm2d", 
                "LIB_DIR/../blas/L1/tests/sw/include/",
                "LIB_DIR/../blas/L2/include/xcl2/"

            ], 
            "symbols": [
                "_GLIBCXX_USE_CXX11_ABI=0"
            ]
        }, 
        "linker": {
            "options": "-luuid -lxrt_coreutil"
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw", 
                "LIB_DIR/L2/include/hw/rtm2d", 
                "LIB_DIR/../blas/L1/include/hw"
            ]
        }
    }, 
    "containers": [
        {
            "name": "rtm", 
            "ldclflags": "--config opts.cfg", 
            "frequency": 250, 
            "accelerators": [
                {
                    "name": "rtmforward", 
                    "location": "LIB_DIR/L2/src/hw/rtm2d/rtmforward.cpp", 
                    "frequency": 250
                }, 
                {
                    "name": "rtmbackward", 
                    "location": "LIB_DIR/L2/src/hw/rtm2d/rtmbackward.cpp", 
                    "frequency": 250
                }
            ]
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": 960
            }
        ], 
        "targets": [
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
