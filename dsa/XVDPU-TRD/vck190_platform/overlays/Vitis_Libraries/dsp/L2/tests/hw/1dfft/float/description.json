{
    "gui": false, 
    "name": "Xilinx 1D FFT Float Reference Design", 
    "description": "", 
    "flow": "vitis", 
    "platform_type": "", 
    "platform_whitelist": [
        "xilinx_u250_xdma_201830_2", 
        "aws-vu9p-f1", 
        "vck190"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u250": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/hls_pre.cfg"
                    ]
                }
            },
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }, 
            "containers": [
                {
                    "name": "fft1DKernel",
                    "ldclflags": "--config PROJECT/conn_u250.cfg"
                }
            ]
        }, 
        "aws-vu9p-f1": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/hls_pre.cfg"
                    ]
                }
            },
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }, 
            "containers": [
                {
                    "name": "fft1DKernel",
                    "ldclflags": "--config PROJECT/conn_aws_f1.cfg"
                }
            ]
        }, 
        "vck190": {
            "host": {
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib", 
                        "${SYSROOT}/opt/xilinx/xrt/lib"
                    ]
                }, 
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include"
                    ], 
                    "options": "--sysroot=$(SYSROOT)"
                }
            }
        }
    }, 
    "launch": [
        {
            "name": "generic launch for sw_emu flows", 
            "target": "sw_emu", 
            "cmd_args": " -xclbin BUILD/fft1DKernel.xclbin -n 100"
        }, 
        {
            "name": "generic launch for hw_emu flows", 
            "target": "hw_emu", 
            "cmd_args": " -xclbin BUILD/fft1DKernel.xclbin -n 2"
        }, 
        {
            "name": "generic launch for hw flows", 
            "target": "hw", 
            "cmd_args": " -xclbin BUILD/fft1DKernel.xclbin -n 1024"
        }
    ], 
    "host": {
        "host_exe": "testFFT.exe", 
        "compiler": {
            "sources": [
                "host/main.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include/hw/vitis_fft/float", 
                "LIB_DIR/L2/include/hw/vitis_fft/float", 
                "LIB_DIR/../utils/L1/include", 
                "host", 
                "kernel", 
                "LIB_DIR/ext/xcl2"
            ], 
            "options": "-O3"
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw/vitis_fft/float", 
                "LIB_DIR/L2/include/hw/vitis_fft/float", 
                "kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/top_module.cpp", 
                    "frequency": 300.0, 
                    "name": "fft1DKernel", 
                    "num_compute_units": 1, 
                    "compute_units": [
                        {
                            "name": "fft1DKernel", 
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "p_inData", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "p_outData", 
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "fft1DKernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 600, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
